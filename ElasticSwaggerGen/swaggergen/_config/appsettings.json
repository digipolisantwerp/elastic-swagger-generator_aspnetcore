{
    "serilog": {
        "writeTo": [
            {
                "name": "LiterateConsole"
            },
            {
                "name": "RollingFile",
                "args": {
                    "pathFormat": "D:/Temp/20170320-keyring-swagger/swaggergen-{Date}.log"
                }
            }
        ]
    },
    "parseOptions": {
        "excludeFiles": [
            "_common.json",
            "cluster.put_settings.json"
        ],
        "commonParamsFilename": "_common.json"
    },
    "swaggerOptions": {
        "excludePaths": [
            "/{index}/{feature}",
            "/_nodes/{metric}"
        ],
        "basePath": "/logging-app1",
        "contactEmail": "da_apie_team@digipolis.be",
        "contactName": "ACPaaS",
        "contactUrl": "https://acpaas.antwerpen.be",
        "apiTitle": "ElasticSearch",
        "apiDescription": "ACPaaS ElasticSearch Logging Engine API.",
        "apiVersion":  "5"
    }

}
